{
  "name": "temp_init",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "docker compose up -d postgres && npx wait-on tcp:5433 && (cd server && npx prisma db push) && node server/index.js",
    "db:reset": "docker compose down -v && docker compose up -d postgres && npx wait-on tcp:5433 && (cd server && npx prisma db push)",
    "db:studio": "cd server && npx prisma studio",
    "build:web": "npx expo export --platform web"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@prisma/client": "^5.22.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "cors": "^2.8.5",
    "expo": "~54.0.31",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-status-bar": "~3.0.9",
    "express": "^5.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "prisma": "^5.22.0",
    "typescript": "~5.9.2",
    "wait-on": "^9.0.3"
  },
  "private": true
}
