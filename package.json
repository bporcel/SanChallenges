{
  "name": "temp_init",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "docker-compose up -d postgres && npx wait-on tcp:5433 && npx prisma db push && node server/index.js",
